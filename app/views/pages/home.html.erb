    </div>
  </div>
  <div class="d-flex">
    <div class="d-flex align-items-center">
      <%= image_tag "person_1.jpg" %>
    </div>
    <div>
      <%= image_tag "person_2.jpg" %>
      <%= image_tag "person_3.jpg" %>
    </div>
  </div>
</div>

<div class="container d-flex card-final">
  <div class="cards-final w-30">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
