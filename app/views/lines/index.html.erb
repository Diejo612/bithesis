<% if current_user.role == 'teacher' %>
  <%= render 'dashboard_teacher' %>
<% else %>
  <% if current_user.admited %>
    <%= render 'dashboard' %>
  <% else %>
    <% if Interview.where(user: current_user).count > 0 %>
       <%= render 'interview_confirmed' %>
    <% else %>
      <%= render 'no_interview' %>
    <% end %>
  <% end %>
<% end %>
